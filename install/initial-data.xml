		<member_name>admin</member_name>
		<name>Administrators</name>
		<ranking>10</ranking>
		<dependents>
			<user>
				<name>admin</name>
				<email>%email%</email>
				<password>%password%</password>
				<group_id>%lastid%</group_id>
			</user>
