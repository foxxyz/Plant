<?xml version="1.0" encoding="utf-8" ?>
<data>
	<controller>
		<name>FeedController</name>
		<dependents>
			<path>
				<parent>1</parent>
				<path>feeds</path>
				<controller_id>%lastid%</controller_id>
				<title>Feeds</title>
				<authentication_required>false</authentication_required>
			</path>
		</dependents>
	</controller>
	<controller>
		<name>PostController</name>
		<dependents>
			<path>
				<parent>1</parent>
				<path>blog</path>
				<controller_id>%lastid%</controller_id>
				<title>Blog</title>
				<authentication_required>false</authentication_required>
			</path>
			<path>
				<parent>3</parent>
				<path>posts</path>
				<controller_id>%lastid%</controller_id>
				<title>Posts</title>
				<authentication_required>true</authentication_required>
				<dependents>
					<controller>
						<name>FilebrowserController</name>
						<dependents>
							<path>
								<parent>%lastid%2</parent>
								<path>browser</path>
								<controller_id>%lastid%</controller_id>
								<title>Filebrowser</title>
								<authentication_required>true</authentication_required>
							</path>
						</dependents>
					</controller>
					<controller>
						<name>PostcategoryController</name>
						<dependents>
							<path>
								<parent>%lastid%2</parent>
								<path>categories</path>
								<controller_id>%lastid%</controller_id>
								<title>Categories</title>
								<authentication_required>true</authentication_required>
							</path>
						</dependents>
					</controller>
					<controller>
						<name>CommentController</name>
						<dependents>
							<path>
								<parent>%lastid%2</parent>
								<path>comments</path>
								<controller_id>%lastid%</controller_id>
								<title>Comments</title>
								<authentication_required>true</authentication_required>
							</path>
						</dependents>
					</controller>
				</dependents>
			</path>
		</dependents>
	</controller>
</data>